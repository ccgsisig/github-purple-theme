{
  "manifest_version": 2,
  "name": "GitHub Prettify",
  "version": "0.0.1",

  "browser_action": {
   "default_icon": "icon.png"
  },

  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [],
      "css": ["content.css"]
    }
  ],
  "description": "A GitHub Universe inspired theme for any GitHub pages.",
  "author": "atsullivan"
}
